@model dynamic

<!-- Header -->
<header>
    <div class="container">
        <div class="intro-text">
            <div class="intro-lead-in">@L("home_index_welcome")</div>
            <div class="intro-heading">@L("home_index_welcome_2")</div>
            <a href="#services" class="page-scroll btn btn-xl">@L("home_index_find_more")</a>
        </div>
    </div>
</header>

<!-- Services Section -->
<section id="services">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">@L("home_index_services")</h2>
                <h3 class="section-subheading text-muted">@L("home_index_services_desc")</h3>
            </div>
        </div>
        <div class="row text-center">
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-globe fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@L("home_index_web_apps")</h4>
                <p class="text-muted">@L("home_index_web_apps_long")</p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-laptop fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@L("home_index_responsive")</h4>
                <p class="text-muted">@L("home_index_responsive_long")</p>
            </div>

            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-code fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@L("home_index_code")</h4>
                <p class="text-muted">@L("home_index_code_long")</p>
            </div>
        </div>
        <div class="row text-center">
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-lock fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@L("home_index_web_security")</h4>
                <p class="text-muted">
                    @L("home_index_web_security_long")
                </p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-database fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@L("home_index_databases")</h4>
                <p class="text-muted">
                    @L("home_index_databases_long")
                </p>
            </div>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="fa fa-android fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">@L("home_index_mobile_apps")</h4>
                <p class="text-muted">
                    @L("home_index_mobile_apps_long")
                </p>
            </div>
        </div>
    </div>
</section>

<!-- About Section -->
<section id="about">
